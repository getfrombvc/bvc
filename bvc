package Input;

import java.io.FileInputStream;

import java.io.FileOutputStream;



import org.apache.poi.ss.usermodel.Cell;

import org.apache.poi.ss.usermodel.Row;

import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class Excelinput

{



 public void excel(String abc) throws Exception

 {

  FileInputStream fis = new FileInputStream("C:\\Users\\sbasired\\eclipse-workspace\\Petco\\src\\Petcodata.xlsx");

  XSSFWorkbook wb = new XSSFWorkbook(fis);

  XSSFSheet s = wb.getSheet("Sheet1");

  Row r = s.createRow(0);

  Cell c = r.createCell(0);

  c.setCellValue(abc);

  FileOutputStream fos = new FileOutputStream("C:\\Users\\sbasired\\eclipse-workspace\\Petco\\src\\Petcodata.xlsx");

  wb.write(fos);

  fos.close();

 }

 public static String cellvalue(int row, int column) throws Exception

 {

  FileInputStream fis = new FileInputStream("C:\\Users\\sbasired\\eclipse-workspace\\Petco\\src\\Petcodata.xlsx");

  XSSFWorkbook wb = new XSSFWorkbook(fis);

  XSSFSheet s = wb.getSheet("Sheet1");

  String data1 = s.getRow(0).getCell(0).getStringCellValue();

  return data1;

 }

}

