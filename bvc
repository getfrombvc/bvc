package savari1;



import java.util.List;

import java.util.concurrent.TimeUnit;





import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;







public class search extends driversetup{

 WebDriver driver;

 @BeforeMethod

 public void test()

 {

 driver=setupChrome();

 }

 @Test

 public void search() throws Exception

 {

 Actions act=new Actions(driver);

 driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);

 driver.findElement(By.xpath("//*[@class='margin-left-20px']")).click();

 WebElement we=driver.findElement(By.xpath("//*[@id='fromCityList']"));

 we.sendKeys("Mumbai");

 Thread.sleep(2000);

 we.sendKeys(Keys.DOWN);

 we.sendKeys(Keys.ENTER);



 WebElement we1=driver.findElement(By.xpath("//*[@class='col-10 col tocityHolder']//input"));

 we1.sendKeys("Pune");

 Thread.sleep(2000);

 //we1.sendKeys(Keys.DOWN);

 we1.sendKeys(Keys.ENTER);

 driver.findElement(By.xpath("//*[@class='ng-tns-c11-1 form-control ui-inputtext ui-widget ui-state-default ui-corner-all']")).click();

 driver.findElement(By.xpath("//*[text()='30']")).click();

 driver.findElement(By.xpath("//*[@id='pickUpTime']")).click();

 driver.findElement(By.xpath("//*[@id='pickUpTime']//option[3]")).click();

 driver.findElement(By.xpath("//*[@class='book-button btn']")).click();

 Thread.sleep(5000);

 List<WebElement>carname=driver.findElements(By.xpath("//*[@class='carBlockFirst']"));

 for(int i=0;i<2;i++)

 {

  System.out.println(carname.get(i).getText());

 }

 List<WebElement>regularPrice=driver.findElements(By.xpath("//*[@class='regularPrice']"));

 for(int i=0;i<2;i++)

 {

  System.out.println(regularPrice.get(i).getText());

 }

 }

 @AfterMethod

 public void close()

 {

 driver.quit();

 }



}



INDIGO:

driversetup:

package indigo1;



import org.openqa.selenium.WebDriver;

import org.openqa.selenium.chrome.ChromeDriver;

import org.openqa.selenium.firefox.FirefoxDriver;



public class driversetup {

 public static String url="https://www.goindigo.in/";

 public WebDriver setupChrome()

 {

 System.setProperty("webdriver.chrome.driver","C:\\selenium code\\chromedriver.exe");

 WebDriver driver=new ChromeDriver();

 driver.navigate().to(url);

 return driver;



 }

 public WebDriver setupFirefox()

 {

 WebDriver driver=new FirefoxDriver();

 driver.navigate().to(url);

 return driver;

}

}

Excel:

package indigo1;



import java.io.FileInputStream;



import org.apache.poi.xssf.usermodel.XSSFCell;

import org.apache.poi.xssf.usermodel.XSSFRow;

import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class excelutils {

 public static String getCellValue(String sheetname,int rowNum,int cellNum)

 {

 String CellValue=null;

 try

 {

 FileInputStream excelfile=new FileInputStream("C:\\mounikaexcel\\TestData.xlsx");

 XSSFWorkbook workbook=new XSSFWorkbook(excelfile);

 XSSFSheet sheet=workbook.getSheet(sheetname);

 XSSFRow row=sheet.getRow(rowNum);

 XSSFCell cell=row.getCell(cellNum);

 CellValue=cell.getStringCellValue();

 }

 catch(Exception e)

 {

  e.printStackTrace();

 }



 return CellValue;

}

}



Search:

package indigo1;



import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;



public class search extends driversetup {

 WebDriver driver;

 @BeforeMethod

 public void test()

 {

 driver=setupChrome();

 }

 @Test(priority=1)

 public void search() throws Exception

 {

 Actions act=new Actions(driver);

 driver.findElement(By.xpath("//*[@for='roundTrip']")).click();

 WebElement we=driver.findElement(By.name("or-src"));

 we.click();

 we.sendKeys(excelutils.getCellValue("Sheet1",3,0));

 Thread.sleep(2000);

 //we.sendKeys(Keys.DOWN);

 we.sendKeys(Keys.ENTER);

 WebElement we1=driver.findElement(By.name("or-dest"));

 we1.sendKeys(excelutils.getCellValue("sheet1",3, 1));

 Thread.sleep(2000);

 //we1.sendKeys(Keys.DOWN);

 we1.sendKeys(Keys.ENTER);

 driver.findElement(By.xpath("//*[@class='form-control or-depart igInitCalendar focus']")).click();

 driver.findElement(By.xpath("//*[@class=' ui-datepicker-week-end ']//*[text()='21']")).click();

 driver.findElement(By.xpath("//*[@class='form-control or-return igInitCalendar focus']")).click();

 driver.findElement(By.xpath("//div[@class='ui-datepicker-group ui-datepicker-group-first']//*[text()='23']")).click();

 driver.findElement(By.name("passenger")).click();

 driver.findElement(By.xpath("//*[@class='passenger-dropdown pax-selection-row']//*[@class='adult-pax-list']//*[@class='pax-plus btn btn-info']")).click();

 //Select sc=new Select(driver.findElement(By.xpath("//*[@class='dropdown bootstrap-select form-control bw-currency-list']//*[@class='filter-option-inner-inner']")));

 //sc.selectByVisibleText("Indian Rupees");



 WebElement we3=driver.findElement(By.xpath("//*[@type='submit']//*[text()='Search Flight']"));

 act.moveToElement(we3).click().perform();





 }

 @AfterMethod

 public void close()

 {

 driver.quit();

 }









 }



