TestNG



package icicbank;



import org.testng.annotations.Test;

import org.testng.annotations.BeforeTest;



import java.io.IOException;



import org.openqa.selenium.By;

import org.openqa.selenium.JavascriptExecutor;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterTest;



public class testing extends dricersetup{

 WebDriver driver;

 @Test

 public void icici() throws InterruptedException, IOException {

  

  //Selecting quarterly payout in drop down

 driver.switchTo().frame("paymframe");

  JavascriptExecutor js=(JavascriptExecutor)driver;

  js.executeScript("window.scrollBy(0,200)");

  WebElement deposit=driver.findElement(By.id("ddlTypeOfFixedDeposit"));

  deposit.click();

  Thread.sleep(1000);

  Select sel=new Select(deposit);

  sel.selectByVisibleText("Quarterly Payout");

  Thread.sleep(1000);

  

  //amount=60000

  WebElement amt=driver.findElement(By.xpath("//*[@id='loanAmount']"));

  amt.clear();

  Thread.sleep(1000);

  amt.sendKeys("60000");

  

  //print maturity and agg value

  String Maturity1=driver.findElement(By.id("spnMaturityValue")).getText();

  String agg1=driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

  System.out.println("Maturity value ="+Maturity1+" Aggregate value= "+agg1);

  Thread.sleep(2000);

  

  

  

  //select tenure radio button

  js.executeScript("window.scrollBy(0,200)");

 driver.findElement(By.xpath("//*[@id='idDays']")).click();

 Thread.sleep(1000);

	

 //enter days=1000

 WebElement days=driver.findElement(By.xpath("//*[@id='tenureDay']"));

 days.clear();

 days.sendKeys("1000");

 Thread.sleep(1000);

	

 //print maturity and agg value

 String Maturity2=driver.findElement(By.id("spnMaturityValue")).getText();

  String agg2=driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

