public class testagain extends driversetup {

 WebDriver driver;

 @Test

 public void search() throws IOException, InterruptedException {

  driver.findElement(By.xpath("//*[@for='roundTrip']")).click();

  Thread.sleep(1000);

  

  WebElement from1=driver.findElement(By.xpath("//*[@name='or-src']"));

  from1.clear();

  Thread.sleep(500);

  from1.click();

  Thread.sleep(500);

  from1.sendKeys(excelsetup.getcellvalue("Sheet1",0,0));

  from1.sendKeys(Keys.ENTER);

  Thread.sleep(1000);

  

  WebElement dest1=driver.findElement(By.name("or-dest"));

  dest1.clear();

  dest1.click();

  dest1.sendKeys(excelsetup.getcellvalue("Sheet1",0,1));

  dest1.sendKeys(Keys.ENTER);

  Thread.sleep(1000);

  

  driver.findElement(By.xpath("//input[@name='or-depart' and @data-parsley-required='true']"));

  JavascriptExecutor js=(JavascriptExecutor)driver;

  js.executeScript("window.scrollBy(0,200)");

  Thread.sleep(2000);

  driver.findElement(By.xpath("//*[@data-year='2020']//a[text()='14']")).click();

  Thread.sleep(2000);

  

  driver.findElement(By.xpath("//input[@name='or-return' and @data-parsley-flightreturndate='true']")).click();

  Thread.sleep(2000);

  driver.findElement(By.xpath("//*[@data-year='2020']//a[text()='15']")).click();

  Thread.sleep(2000);

  

  

   WebElement passenger= driver.findElement(By.name("passenger"));

   passenger.click();

   Thread.sleep(2000);

    driver.findElement(By.xpath("//li[@class='adult-pax-list']/div[1]/button[2]")).click();

 Thread.sleep(1000);

	

 driver.findElement(By.xpath("//*[@class='passenger-done-blck']//*[text()='Done']")).click();

 Thread.sleep(2000);



//	TakesScreenshot scrshot=((TakesScreenshot)driver);

// File srcFile=scrshot.getScreenshotAs(OutputType.FILE);

// FileUtils.copyFile(srcFile,new File("C:\\Users\\J.Siddayya swami\\eclipse-workspace\\Selenium_SBA\\indigo1.png"));



   WebElement ele=driver.findElement(By.xpath("//*[@type='submit']//*[text()='Search Flight']"));

   Thread.sleep(2000);

  // js.executeScript("arguments[0].click();",ele);

   Actions actions = new Actions(driver);

   actions.moveToElement(ele).click().perform();

   Thread.sleep(1000);

    

    String fltnum=driver.findElement(By.xpath("//*[@id='6E~6005~~~CJB~01/14/202007:35~MAA~01/14/202008:35~~']//*[@class='flightNo']")).getText();

    System.out.println(fltnum);

    excelsetup.setcellvalue("Sheet2", fltnum);

    

    

  

 }

 @BeforeTest

