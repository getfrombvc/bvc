package icicbank;



import org.openqa.selenium.WebDriver;

import org.openqa.selenium.chrome.ChromeDriver;



public class dricersetup {

 String url="https://www.icicibank.com/calculators/fd-calculator.html";

 WebDriver driver;

 public WebDriver chromesetup() {

 System.setProperty("webdriver.chrome.driver", "C:\\\\selinium\\\\cucumber jars\\\\chromedriver.exe");

 WebDriver driver=new ChromeDriver();

 driver.navigate().to(url);

 driver.manage().deleteAllCookies();

 return driver;





 }



}

