 driver.manage().window().maximize();

 }

 @Test(priority=1)

 public void search() throws Exception

 {

 driver.findElement(By.xpath("//a[@class='btn btn-success']")).click();

 Thread.sleep(2000);

 WebElement we=driver.findElement(By.xpath("//input[@id='email']"));

 //we.click();

 Thread.sleep(2000);

 we.sendKeys(excelutils.getCellValue("Sheet1",1,0));

 Thread.sleep(2000);

 we.sendKeys(Keys.DOWN);

 we.sendKeys(Keys.ENTER);

 //String str=driver.findElement(By.xpath("//*[@id=\"deorg_form\"]/div[3]/div")).getText();

 //System.out.println("Error Message:" +str);

 String str=driver.findElement(By.xpath("//div[text()='Please enter a valid email address']")).getText();

 System.out.println("Error Message:" +str);

 WebElement we1=driver.findElement(By.xpath("//*[@id='country']"));

 we1.sendKeys(excelutils.getCellValue("Sheet1",2,0));

 Thread.sleep(2000);

 we1.sendKeys(Keys.DOWN);

 we1.sendKeys(Keys.ENTER);

 driver.findElement(By.xpath("//*[text()='Log In']")).click();

 driver.findElement(By.xpath("//*[@name='rememberUn']")).click();

 }

 @AfterMethod

 public void close()

 {

