 System.out.println("first name= "+name);

 Thread.sleep(1000);







  

//click on the product

 driver.findElement(By.xpath("//*[@class='h3 product-title']//*[@href='https://www.marshallspetzone.com/dry-food/5654-versele-laga-junior-om-no-maize-25-kg.html']")).click();

 Thread.sleep(1000);

  

  

  

