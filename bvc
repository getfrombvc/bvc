Excell



package icicbank;



import java.io.FileInputStream;

import java.io.FileNotFoundException;

import java.io.FileOutputStream;

import java.io.IOException;



import org.apache.poi.xssf.usermodel.XSSFCell;

import org.apache.poi.xssf.usermodel.XSSFRow;

import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class excelcode {

 static String cellvalue=null;

 public static String getcellvalue(String sheetname, int rownum, int colnum) throws IOException {

 FileInputStream file;

 file=new FileInputStream("C:\\Users\\J.Siddayya swami\\eclipse-workspace\\Selenium_SBA\\icici.xlsx");

 XSSFWorkbook workbook=new XSSFWorkbook(file);

 XSSFSheet sheet=workbook.getSheet(sheetname);

 XSSFRow row=sheet.getRow(rownum);

 XSSFCell cell=row.getCell(colnum);

 cellvalue=cell.getStringCellValue();

 return cellvalue;

 }

 public static void setcellvalue(String sheetname, String maturity1, String Agg1, String maturity2,String Agg2) throws IOException {

 // TODO Auto-generated method stub

 FileInputStream file;

 file=new FileInputStream("C:\\Users\\J.Siddayya swami\\eclipse-workspace\\Selenium_SBA\\icici.xlsx");



 //maturity1

 XSSFWorkbook workbook=new XSSFWorkbook(file);

 XSSFSheet sheet=workbook.getSheet(sheetname);

 XSSFRow row1=sheet.getRow(0);

 XSSFCell cell1=row1.getCell(0);

 cell1.setCellValue(maturity1);



 //agg1

 XSSFRow row2=sheet.getRow(0);

 XSSFCell cell2=row2.getCell(1);

 cell2.setCellValue(Agg1);



 //maturity2

 XSSFRow row3=sheet.getRow(0);

 XSSFCell cell3=row3.getCell(0);

 cell3.setCellValue(maturity2);



 //Agg2

 XSSFRow row4=sheet.getRow(0);

 XSSFCell cell4=row4.getCell(0);

 cell4.setCellValue(Agg2);



 FileOutputStream fos=new FileOutputStream("C:\\Users\\J.Siddayya swami\\eclipse-workspace\\Selenium_SBA\\icici.xlsx");

 workbook.write(fos);

 fos.close();



 }



}

