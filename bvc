Home Page Class





package goibibo.pom;



import java.io.IOException;

import java.util.concurrent.TimeUnit;



import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.FindBy;

import org.openqa.selenium.support.PageFactory;



public class Homepage {

 driverclass ds;

 testcase tst;

 WebDriver driver;

 @FindBy(xpath="//*[@placeholder='From']")

 WebElement Frombutton;

 @FindBy(xpath="//*[@placeholder='Destination']")

 WebElement destbut;

 @FindBy(xpath="//*[@id='departureCalendar']")

 WebElement depcal;

 @FindBy(xpath="//*[@class='DayPicker-NavButton DayPicker-NavButton--next']")

 WebElement nextmnth;

 @FindBy(xpath="//*[text()='13']")

 WebElement depdate;

 @FindBy(xpath="//*[@id='returnCalendar']")

 WebElement retcal;

 @FindBy(xpath="//*[text()='14']")

 WebElement retdate;

 @FindBy(xpath="//*[text()='1 Traveller, ']")

 WebElement pass;

 @FindBy(xpath="//*[@id='adultPaxPlus']")

 WebElement passengers;

 @FindBy(xpath="//*[text()='SEARCH']")

 WebElement search;

 @FindBy(xpath="//*[@class='db greyLt ico11 padT3']")

 WebElement flight;

 @FindBy(xpath="//*[@id='pax_close']")

 WebElement cls;









 public Homepage(WebDriver driver1)

 {

 this.driver=driver1;

 PageFactory.initElements(driver, this);

 }



 public void frombutton(String from) throws InterruptedException

 {



 driver.manage().timeouts().implicitlyWait(5,TimeUnit.SECONDS);

 Frombutton.click();

 Thread.sleep(5000);

 Frombutton.sendKeys(from);

 Thread.sleep(1000);

 Frombutton.sendKeys(Keys.DOWN);

 Thread.sleep(1000);

 Frombutton.sendKeys(Keys.ENTER);



 }



 public void destbutton(String dest) throws InterruptedException {

 // TODO Auto-generated method stub

 destbut.sendKeys(dest);

 Thread.sleep(5000);

 destbut.sendKeys(Keys.DOWN);





 destbut.sendKeys(Keys.ENTER);







 }



 public void depdate() throws InterruptedException {

 // TODO Auto-generated method stub

 depcal.click();

 Thread.sleep(5000);

 nextmnth.click();

 Thread.sleep(1000);

 depdate.click();

 }



 public void retdate() throws InterruptedException {

 // TODO Auto-generated method stub

 retcal.click();

 Thread.sleep(5000);

 retdate.click();

 }



 public void passenger() {

 // TODO Auto-generated method stub

 pass.click();

 passengers.click();

 cls.click();

 }



 public void search() throws InterruptedException {

 // TODO Auto-generated method stub

 //this.driver=tst.driver1;





 Actions ac = new Actions(driver);

 ac.moveToElement(search);

 ac.click().build().perform();





 }



 public void flight() throws InterruptedException, IOException {

 // TODO Auto-generated method stub

 //WebDriver driver = new ChromeDriver();

 Thread.sleep(8000);

 //this.driver=tst.driver1;

 java.util.List<WebElement> flt =driver.findElements(By.xpath("//*[@class='db greyLt ico11 padT3']"));

 //System.out.println("size is :"+flt.size());

 String a[] = new String[5];

 for(int i=0;i<5;i++)

 {

  a[i] = flt.get(i).getText();

 }

 System.out.println(a);

 excelcode.setcellvalue("Sheet2",a);





 }











}

