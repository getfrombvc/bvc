10.Pedigree

TestNg



package Pedigree;

import org.testng.annotations.Test;

import org.testng.annotations.BeforeTest;

import java.io.File;

import java.io.IOException;

import java.util.HashSet;

import java.util.Set;

import org.apache.commons.io.FileUtils;

import org.openqa.selenium.By;

import org.openqa.selenium.OutputType;

import org.openqa.selenium.TakesScreenshot;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.testng.Reporter;

import org.testng.annotations.AfterTest;

public class Automation extends DriverSetup {

 WebDriver driver;

 public static String ArticleCount;

 @Test

 public void count() throws IOException, InterruptedException {

// Clicking the search button

 WebElement srch = driver.findElement(By.xpath("//div[@class='search']/div/a"));

 srch.click();

 Thread.sleep(8000);

 Reporter.log("Search icon has been clicked");

// Entering the value 'Dog' from Excel

 WebElement val = driver.findElement(By.name("search"));

 val.click();

 Thread.sleep(3000);

 String valEx = ExcelUtility.getValue(1, 0);

 val.sendKeys(valEx);

 Thread.sleep(3000);

 Reporter.log("The value 'Dog' has been entered");

// Retrieving the Article count

 WebElement art = driver.findElement(By.className("ng-binding"));

 ArticleCount = art.getText();

 ExcelUtility excel = new ExcelUtility();

 excel.setValue(ArticleCount);

 Reporter.log("The Article Count has been stored");

 if (val==srch){

 System.out.println("Equals");

 }

 else {

 System.out.println("Not equal");

 }

// Clicking the second article from the list

// Instantiating Action Class

 //This line got the index of the current window

 String homewindow =driver.getWindowHandle();

// Actions action = new Actions(driver);

 WebElement art2 = driver.findElement(By.xpath("(//li[@class='ng-scope']/a)[2]"));

 art2.click();

// action.moveToElement(art2).perform();

// action.moveToElement(art2).click().perform();

 Thread.sleep(8000);

 Reporter.log("Navigated to second article");

 driver.close();

 Set<String> allwindow =new HashSet<String>();

 allwindow=driver.getWindowHandles();

 for(String windowitr: allwindow){

 System.out.println(windowitr);

 if(homewindow!=windowitr)

 {

 driver.switchTo().window(windowitr);

 System.out.println(driver.getTitle());

 if(driver.getTitle().contains("Which foods should my dog avoid?"))

 {

 // Clicking on logo in the newly opened tab

 WebElement logo = driver.findElement(By.id("Header_imgLogo"));

 logo.click();

 Thread.sleep(3000);

 Reporter.log("The homepage is displayed ");

 }

 }

 }

// Taking Screenshot of the page

 TakesScreenshot snap = (TakesScreenshot)driver;

 File file = snap.getScreenshotAs(OutputType.FILE);

 FileUtils.copyFile(file, new File(System.getProperty("User.dir") + "\\src\\Screenshot.png"));

 }

 @BeforeTest

 public void beforeTest() throws InterruptedException {

// Initializing the driver setup

// choosing any one of the browser

 driver = chrome();

// driver = ie();

// driver = firefox();

 Reporter.log("Browser has been launched successfully");

 }

 @AfterTest

 public void afterTest() {

 //closing the browser after testing

 driver.close();

 Reporter.log("Browser has been closed");

 }

}

