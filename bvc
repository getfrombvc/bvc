package goibibo2;



import java.util.concurrent.TimeUnit;



import org.openqa.selenium.By;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;



public class search extends driversetup {

WebDriver driver;

@BeforeMethod

public void test()

{

 driver=setupChrome();

 driver.manage().window().maximize();

}

@Test(priority=1)

public void search() throws InterruptedException

{

 driver.manage().timeouts().implicitlyWait(10,TimeUnit.SECONDS);

 driver.findElement(By.id("roundTrip")).click();

 WebElement we=driver.findElement(By.xpath("//*[@id='gosuggest_inputSrc']"));

 we.click();

 we.sendKeys(excelutils.getCellValue("Sheet1",0,0));

 Thread.sleep(2000);

 we.sendKeys(Keys.DOWN);

 we.sendKeys(Keys.ENTER);

 WebElement we1=driver.findElement(By.xpath("//*[@id='gosuggest_inputDest']"));

 we1.click();

 we1.sendKeys(excelutils.getCellValue("Sheet1",0,1));

 Thread.sleep(2000);

 we1.sendKeys(Keys.DOWN);

 we1.sendKeys(Keys.ENTER);

 driver.findElement(By.id("departureCalendar")).click();

 driver.findElement(By.xpath("//*[text()='30']")).click();

 driver.findElement(By.id("returnCalendar")).click();

 driver.findElement(By.xpath("//*[text()='31']")).click();

 driver.findElement(By.xpath("//*[text()='1 Traveller, ']")).click();

 driver.findElement(By.xpath("//div[@class='number-spinner number-spinnerFil']//*[@class='fl plusSpin brdrTpBtmRt']")).click();

 driver.findElement(By.xpath("//*[@id='gi_class']/option[1]")).click();

 driver.findElement(By.xpath("//*[@value='Search']")).click();

 Thread.sleep(2000);

 String str=driver.findElement(By.xpath("//span[@class='db greyLt ico11 padT3'][1]")).getText();

 //System.out.println("first flight name:" +str);

}

@AfterMethod

public void close()

{

 driver.quit();

}

