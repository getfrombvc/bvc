indigo.java

package goindigo.pom;



import java.io.IOException;



import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.support.FindBy;

import org.openqa.selenium.support.PageFactory;



//import goibibo.pom.driverclass;



public class indigo {

 testcase tcs;

 //driverclass dcs;

 WebDriver driver;



 @FindBy(xpath="//*[@for='roundTrip']")

 WebElement btnradio;



 @FindBy(name="or-src")

 WebElement txtfrom;



 @FindBy(xpath="//*[@name='or-dest']")

 WebElement txtdest;



 @FindBy(xpath="//*[@name='or-depart']")

 WebElement txtdeparture;



 @FindBy(xpath="(//*[text()='14'])[1]")

 WebElement numdeparture ;



 @FindBy(xpath="//*[@name='or-return']")

 WebElement txtreturn;



 @FindBy(xpath="//*[text()='15'][1]")

 WebElement numreturn;



 @FindBy(xpath="//*[@placeholder='pax-class']")

 WebElement txtpassenger;



 @FindBy(xpath="//*[@class='modal-body']//*[@class='adult-pax-list']//*[@class='icon-plus']")

 WebElement btnplus;



 @FindBy(xpath="//*[@class='modal-footer'][1]")

 WebElement btndonepass;



 @FindBy(xpath="//*[@class=\"hp-src-btn\"]")

 WebElement btnsearch;



 @FindBy(xpath="//*[text()='6005']")

 WebElement numflt;



 public indigo(WebDriver driver1) {



 this.driver=driver1;

 PageFactory.initElements(driver,this);

 }



 public void radiobutton() {

 btnradio.click();

 }



 public void from(String frm1) throws InterruptedException {

 txtfrom.click();

 Thread.sleep(5000);

   txtfrom.sendKeys(frm1);

   Thread.sleep(1000);

   txtfrom.sendKeys(Keys.ENTER);

   Thread.sleep(1000);

 }



 public void to(String dst1) throws InterruptedException {

 txtdest.click();

 Thread.sleep(1000);

 txtdest.sendKeys(dst1);

 Thread.sleep(1000);

 txtdest.sendKeys(Keys.ENTER);

 Thread.sleep(1000);

 }



 public void departure() throws InterruptedException {

 txtdeparture.click();

 Thread.sleep(1000);

 numdeparture.click();

 }



 public void returndate() throws InterruptedException {

 txtreturn.click();

 Thread.sleep(1000);

 numreturn.click();

 }



 public void passngr() {

 txtpassenger.click();

 btnplus.click();

 btndonepass.click();

 }



 public void searchflight() {

 btnsearch.click();

 }



 public void flightnum() throws IOException {

 System.out.println("Flight number"+numflt);

 String flight=numflt.getText();

 excelsetup.setcellvalue("Sheet2",flight);

 }



}

