DriverSetup



package Pedigree;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.chrome.ChromeDriver;

import org.openqa.selenium.firefox.FirefoxDriver;

import org.openqa.selenium.ie.InternetExplorerDriver;

public class DriverSetup {

 public static String path1 = System.getProperty("user.dir") + "\\chromedriver.exe";

 public static String path2 = "E:\\SeleniumSBA\\src\\IEDriverServer.exe";

 public static String path3 = "E:\\SeleniumSBA\\src\\geckodriver.exe";

 public static String URL = "https://www.pedigree.in";

 public static WebDriver chrome() throws InterruptedException {

 System.setProperty("webdriver.chrome.driver", path1);

 WebDriver driver = new ChromeDriver();

 driver.manage().window().maximize();

 driver.get(URL);

 Thread.sleep(8000);

 return driver;

 }

 public WebDriver ie() throws InterruptedException {

 System.setProperty("webdriver.ie.driver", path2);

 WebDriver driver = new InternetExplorerDriver();

 driver.manage().window().maximize();

 driver.get(URL);

 return driver;

 }

 public WebDriver firefox() {

 System.setProperty("webdriver.gecko.driver", path3);

 WebDriver driver = new FirefoxDriver();

 driver.manage().window().maximize();

 driver.get(URL);

 return driver;

 }

}

----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

Exclesheet



package Pedigree;

import java.io.File;

import java.io.FileInputStream;

import java.io.FileOutputStream;

import java.io.IOException;

import org.apache.poi.xssf.usermodel.XSSFCell;

import org.apache.poi.xssf.usermodel.XSSFRow;

import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;

public class ExcelUtility {

 static FileInputStream fis;

 static XSSFWorkbook wb;

 static XSSFSheet sh;

 static XSSFRow row;

 static XSSFCell cell;

 FileOutputStream fos;

 static String filePath1 = System.getProperty("user.dir") + "\\src\\TestData.xlsx";

 static String filePath2 = System.getProperty("user.dir") + "\\src\\ExcelOutput.xlsx";

 public static String getValue(int rowNum, int cellNum) throws IOException {

 String cellValue = null;

 fis = new FileInputStream(filePath1);

 wb = new XSSFWorkbook(fis);

 sh = wb.getSheetAt(0);

 row = sh.getRow(rowNum);

 cell = row.getCell(cellNum);

 cellValue = cell.getStringCellValue();

 return cellValue;

 }

 public void setValue(String artCnt) throws IOException {

 wb = new XSSFWorkbook();

 sh = wb.createSheet("Sheet1");

 row = sh.createRow(0)

 row.createCell(0).setCellValue("Article Count");

 row = sh.createRow(1);

 row.createCell(0).setCellValue(artCnt);

 fos = new FileOutputStream(new File(filePath2));

 wb.write(fos);

 fos.close();

 System.out.println("Values are written successfully");

 }

}

