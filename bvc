package icicibank;



import java.util.concurrent.TimeUnit;



import org.openqa.selenium.By;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;



public class search extends driversetup {

 WebDriver driver;

 @BeforeMethod

 public void test()

 {

 driver=setupChrome();

 driver.manage().window().maximize();

 }

@Test(priority=1)

public void search() throws Exception

{

 driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);

 //Select se=new Select(driver.findElement(By.xpath("//*[@id='ddlTypeOfFixedDeposit']")));

 //se.selectByVisibleText("Quarterly Payout");



 driver.switchTo().frame("paymframe");

 Actions act=new Actions(driver);

 driver.findElement(By.xpath("//*[@id='ddlTypeOfFixedDeposit']")).click();

 driver.findElement(By.xpath("//option[@id='quarterly_payout']")).click();

 WebElement we=driver.findElement(By.xpath("//*[@id='loanAmount']"));

 we.clear();

 Thread.sleep(2000);

 we.sendKeys("60000");

 String str=driver.findElement(By.xpath("//span[@id='spnMaturityValue']")).getText();

 System.out.println("maturity value1:" +str);

 String str1=driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

 System.out.println("aggregate interest value1:" +str1);

 driver.findElement(By.xpath("//input[@id='idDays']")).click();

 WebElement we1=driver.findElement(By.xpath("//input[@class='textBox depositamt sliderText1' and @id='tenureDay']"));

 we1.clear();

 Thread.sleep(2000);

 we1.sendKeys("1000");

 String str2=driver.findElement(By.xpath("//span[@id='spnMaturityValue']")).getText();

 System.out.println("maturity value:" +str2);

 String str3=driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

 System.out.println("aggregate interest value:" +str3);

 driver.switchTo().parentFrame();

 WebElement logo=driver.findElement(By.xpath("//a[@class='Logo']"));

 act.moveToElement(logo).click().perform();



}

@AfterMethod

public void close()

{

 driver.quit();

}

}



